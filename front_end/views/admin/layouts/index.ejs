<style>
    .loader {
        position: fixed;
        top: 0;
        left: 0;
        right: 0;
        background: #eee;
        bottom: 0;
        z-index: 1000;
        display: flex;
        justify-content: center;
        align-items: center;
    }

    .loader i {
        font-size: 70px;
        color: #3C91E6;
    }
</style>

<body>
    <div class="loader" style="display:none;">
        <i class='bx bx-loader-circle bx-spin bx-rotate-90'></i>
    </div>
    <%- include partials/sidebar %>

    <section id="content">
        <%- include partials/navbar %>
        <%- body %>
    </section>

    <!-- Bootstrap JS Bundle with Popper -->
